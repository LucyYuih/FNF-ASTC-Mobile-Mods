import flixel.FlxG;
import funkin.util.MathUtil;
import funkin.play.PlayState;
import funkin.play.event.SongEvent;
import funkin.modding.module.Module;
import funkin.data.event.SongEventSchema;
class AddCameraZoom extends SongEvent {

    function new() {
        super('AddCameraZoom');
    }

    public override function getTitle():String {
        return 'Add Camera Zoom';
    }

    public override function getEventSchema() {
        return [
            {
                name: 'cameraType',
                title: 'Camera',
                type: 'enum',
                defaultValue: 'both',
                keys: [
                    "HUD Camera" => 'hud',
                    "Game Camera" => 'game',
                    "Both" => 'both'
                ]
            },
            {
                name: 'intensity',
                title: 'Intensity',
                type: 'float',
                defaultValue: '0.05',
            }
        ];
    }
    
    override function handleEvent(data):Void {
    }
}